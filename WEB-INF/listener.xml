<?xml version="1.0" encoding="UTF-8"?>
<!-- root element, all properties must be under this element -->
<listeners>
	<listener>
		<name>WorkOrder Create Listener </name>
		<description>Create details of work order processes</description>
		<class>com.lohool.jproject.xwohi.listener.WorkOrderListener</class>
		<listened-component>com.lohool.jproject.xwohi.po.WorkOrder</listened-component>
		<listened-action>Create</listened-action>
	</listener>
	<listener>
		<name>WorkFlowProcess Update Listener </name>
		<description>Set current processor to the workorder</description>
		<class>com.lohool.jproject.xwohi.listener.WorkFlowProcessListener</class>
		<listened-component>com.lohool.jproject.xwohi.po.WorkFlowProcess</listened-component>
		<listened-action>Update</listened-action>
	</listener>
</listeners>