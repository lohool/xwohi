 <constant name="struts.ui.theme" value="simple" />  
 -->   
 <constant name="struts.i18n.encoding" value="UTF-8" /> 
 <constant name="struts.multipart.maxSize" value="10701096"/>  



	<include file="struts-default.xml" />

	<package extends="struts-default" name="action-default">
	  <global-results>
		<result type="dispatcher" name="access_rejected">/notlimit.jsp</result>
		<result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="input">/uniqueError.jsp</result>
      </global-results>
   </package>

	<package extends="action-default" name="default">
      <default-interceptor-ref name="defaultStack" />

	  	<action name="Login" class="Login">
			<result name="error" type="dispatcher">/index.jsp</result>
			<result name="input" type="dispatcher">/index.jsp</result>
			<result name="success" type="dispatcher">/win/index.jsp</result>
		</action>
	  	<action name="Logout" class="Logout">
			<result name="success" type="dispatcher">/index.jsp</result>
		</action>
		<action name="Upload" class="UpLoad" >
		  	<result name="success">/Knowledge/UploadSuccess.jsp</result>
		</action>
