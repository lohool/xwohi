   <package name="Operator" extends="action-default" namespace="/Operator">
      <default-interceptor-ref name="exceptionStack" />
      <action name="OperatorList" class="BasicList">
         <result name="success">/Operator/OperatorList.jsp</result>
      </action>
      <action name="OperatorSelectList" class="BasicList">
         <result name="success">/Operator/OperatorSelector/OperatorList.jsp</result>
      </action>
	  <action name="OperatorAdd" class="BasicAdd">
         <result type="redirect" name="success">/Operator/CloseRefreshList.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="OperatorDelete" class="BasicModify">
         <result type="dispatcher" name="success">/Operator/CloseRefreshList.jsp</result>
      </action>
      <action name="OperatorInfo" class="BasicInfo">
         <result type="dispatcher" name="success">/Operator/OperatorInfo.jsp</result>
      </action>
	  <action name="OperatorModify" class="BasicModify">
         <result type="redirect" name="success">/Operator/CloseRefreshList.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="OperatorInput" class="BasicInput">
         <result type="dispatcher" name="success">/Operator/OperatorAdd.jsp</result>
      </action>
		<action name="OperatorModPwd" class="OperatorModPwd">
			<result name="success" type="dispatcher">/Maintain/closeDialog.jsp</result>
			<interceptor-ref name="validationStack" /> 
		</action>
      <action name="MyselfInfo" class="MyselfInfo">
         <result type="dispatcher" name="success">/Operator/MyselfInfo.jsp</result>
      </action>

   </package>

   <package name="Role" extends="action-default" namespace="/Role">
      <default-interceptor-ref name="exceptionStack" />
      <action name="RoleList" class="BasicList">
         <result type="dispatcher" name="success">/Operator/RoleList.jsp</result>
      </action>
      <action name="RoleAdd" class="BasicAdd">
         <result type="dispatcher" name="success">/Operator/CloseRoleDialog.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="RoleDelete" class="BasicDelete">
         <result type="redirect" name="success">/Operator/RoleRefreshList.jsp</result>
      </action>
      <action name="RoleInfo" class="BasicInfo">
         <result type="dispatcher" name="success">/Operator/RoleInfo.jsp</result>
      </action>
      <action name="RoleDataPermission" class="BasicInfo">
         <result type="dispatcher" name="success">/Operator/RoleDataPermission.jsp</result>
      </action>
	  <!--
      <action name="RoleDataPermissionModify" class="RoleDataPermissionModify">
         <result type="dispatcher" name="input">/Operator/RoleDataPermission.jsp</result>
         <result type="redirect" name="success">/Operator/RoleList.action</result>
         <interceptor-ref name="validationStack" />
      </action>
	  -->
      <action name="RoleModify" class="BasicModify">
         <result type="dispatcher" name="success">/Operator/CloseRoleDialog.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="RoleInput" class="BasicInput">
         <result type="dispatcher" name="success">/Operator/RoleAdd.jsp</result>
      </action>
   </package>


   <package name="Resource" extends="struts-default" namespace="/Resource">
      <default-interceptor-ref name="exceptionStack" />
      <action name="ResourceList" class="BasicList">
         <result type="dispatcher" name="success">/Operator/ResourceList.jsp</result>
      </action>
   </package>
   <package name="Privilege" extends="struts-default" namespace="/Privilege">
      <default-interceptor-ref name="exceptionStack" />
      <action name="PrivilegeList" class="BasicList">
         <result type="dispatcher" name="success">/Operator/PrivilegeList.jsp</result>
      </action>
   </package>

   <package name="Department" extends="struts-default" namespace="/Department">
      <default-interceptor-ref name="exceptionStack" />
      <action name="DepartmentList" class="BasicList">
         <result type="dispatcher" name="success">/Operator/DepartmentList.jsp</result>
      </action>
      <action name="OperatorSelDepartmentList" class="BasicList">
         <result type="dispatcher" name="success">/Operator/OperatorSelector/DepartmentList.jsp</result>
      </action>
      <action name="DepartmentAdd" class="BasicAdd">
         <result type="redirect" name="success">/Operator/CloseDepartmentDialog.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="DepartmentDelete" class="BasicDelete">
         <result type="redirect" name="success">/Department/DepartmentList.action</result>
      </action>
      <action name="DepartmentInfo" class="BasicInfo">
         <result type="dispatcher" name="success">/Operator/DepartmentInfo.jsp</result>
      </action>
      <action name="DepartmentModify" class="BasicModify">
         <result type="redirect" name="success">/Operator/CloseDepartmentDialog.jsp</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="DepartmentInput" class="BasicInfo">
         <result type="dispatcher" name="success">/Operator/DepartmentAdd.jsp</result>
      </action>
   </package>


   <package name="NotePad" extends="struts-default" namespace="/NotePad">
      <default-interceptor-ref name="exceptionStack" />
      <action name="NotePadList" class="BasicList">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="success">/NotePad/NotePadList.jsp</result>
      </action>
      <action name="NotePadAdd" class="BasicAdd">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="input">/NotePad/NotePadAdd.jsp</result>
         <result type="chain" name="success">NotePadList</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="NotePadDelete" class="BasicDelete">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="chain" name="success">NotePadList</result>
      </action>
      <action name="NotePadInfo" class="BasicInfo">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="success">/NotePad/NotePadInfo.jsp</result>
      </action>
      <action name="NotePadModify" class="BasicModify">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="input">/NotePad/NotePadInfo.jsp</result>
         <result type="redirect" name="success">/NotePad/NotePadList.action?page=${page}&amp;pageSize=${pageSize}</result>
         <interceptor-ref name="validationStack" />
      </action>
      <action name="NotePadInput" class="BasicInput">
         <result type="dispatcher" name="error">/error.jsp</result>
         <result type="dispatcher" name="success">/NotePad/NotePadAdd.jsp</result>
      </action>
   </package>



</struts>
